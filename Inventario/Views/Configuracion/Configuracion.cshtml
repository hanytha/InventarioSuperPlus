                <div class="modal-body">
                    <h4>REGISTRO CLASIFICACIÓN</h4>
                    <div class="progress">
                        <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuemin="0" aria-valuemax="100"></div>
                    </div>
                    <form id="regiration_form" novalidate method="post" onsubmit="return validarFormularioCategorias()">
                        <fieldset>
                            <!--Los pasos del modal-->

                            <h4>PASO 1</h4>
                            <div class="form-group">
                                <label for="TxtClasificacion">Tipo de clasificación</label>
                                <input type="text" class="form-control limpiar obligatorio" name="data[TxtClasificacion]" id="TxtClasificacion" placeholder="Clasificación">
                            </div>
                            <button id="btnGuardar" type="submit" value="Submit" class="btn btn-success " onclick=" GuardarCategoria()">Guardar</button>
                        </fieldset>
                    </form>
                </div>
                <!-- pie del modal-->
                <div class="modal-footer">
                    <button type="button" id="btnCancelar" class="btn btn-danger" data-dismiss="modal">Cancelar</button>
                </div>
            </div>
        </div>
    </div>
</div>
